{
  "name": "fullstack-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "analyze": "yarn-or-npm build:with-stats && yarn-or-npm start:analyzer",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "build:with-stats": "cross-env NODE_ENV=production webpack --config config/webpack.config.js/client.prod.js --json > build/client/static/bundle-stats.json",
    "build-hx-server": "haxe build-server.hxml && touch finished-building-hx-server",
    "build-hx-client": "haxe build-client.hxml && touch finished-building-hx-client",
    "clean:transpiled": "rimraf build/transpiled",
    "depgraph": "depcruise -c .dependency-cruiser.js --exclude '^node_modules' --output-type dot src | dot -T svg > dependency-graph.svg",
    "i18n:scan": "cross-env NODE_ENV=i18n yarn-or-npm transpile && yarn-or-npm i18next-scanner './build/transpiled/**/*.js' && yarn clean:transpiled",
    "link-react": "yarn-or-npm link react react-dom",
    "lint": "concurrently \"yarn-or-npm lint:js\" \"yarn-or-npm lint:css\"",
    "lint:js": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:css": "stylelint src/**/*.css",
    "lint:deps": "depcruise -c .dependency-cruiser.js src/",
    "plop": "plop",
    "start": "concurrently \"nodemon --verbose -e hx --watch src/hx --exec \\\"concurrently 'npm run build-hx-server' 'npm run build-hx-client' && wait-on finished-building-hx-server finished-building-hx-client && rm finished-building-hx-client finished-building-hx-server\\\"\" \"npm run dev\"",
    "dev": "cross-env NODE_ENV=development node scripts/start.js",
    "start:analyzer": "webpack-bundle-analyzer build/client/static/bundle-stats.json",
    "test": "node scripts/test.js --env=jsdom",
    "test:update": "yarn-or-npm test --updateSnapshot",
    "transpile": "yarn-or-npm babel -d build/transpiled ./src --extensions .es6,.js,.es,.jsx,.mjs,.ts,.tsx --ignore **/*.d.ts",
    "tsc": "tsc --noEmit"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "fullstack",
    "express",
    "express4",
    "node"
  ],
  "author": "Marcelo Serpa",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/polyfill": "^7.2.5",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.3.0",
    "body-parser": "^1.18.3",
    "chance": "^1.0.18",
    "class-autobind": "^0.1.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-manifest-helpers": "^0.6.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "mobx-utils": "^5.4.1",
    "nodemon": "^1.19.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.11",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/dom-inputevent": "^1.0.5",
    "@types/enzyme": "^3.1.15",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.12",
    "@types/node": "^12.7.2",
    "@types/react": "^16.7.18",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/webpack-env": "^1.14.0",
    "autoprefixer": "^9.6.1",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.3",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "concurrently": "^4.1.0",
    "core-js": "^3.2.1",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.4",
